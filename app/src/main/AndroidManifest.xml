<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilessu">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".group_page.Activity_group"></activity>
        <activity android:name=".choose_course.Activity_course" />
        <activity android:name=".choose_day_evening.Activity_day_evening" />
        <activity android:name=".menu_page.Activity_menu" />
        <activity android:name=".student_faculty_page.Activity_student_faculty" />
        <activity android:name=".teacher_page.ActivityFindTeacher" android:windowSoftInputMode="adjustPan"
            />

        <activity android:name=".teacher_or_student_page.Activity_teacher_or_student" />
        <activity android:name=".newspage.ActivityNews">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>